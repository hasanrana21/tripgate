profile...\" />\r\n      </div>\r\n      <div class=\"buttons\">\r\n        <button @click=\"sortAsc\">▲</button>\r\n        <button @click=\"sortDesc\">▼</button>\r\n      </div>\r\n\r\n      <ProfileCard\r\n        v-for=\"(profile, index) in searchProfile\"\r\n        :key=\"index\"\r\n        :profile=\"profile\"\r\n        class=\"profile\"\r\n      />\r\n\r\n      <div class=\"icons-note\">\r\n        Icons made by\r\n        <a href=\"https://www.flaticon.com/authors/freepik\" title=\"Freepik\"\r\n          >Freepik</a\r\n        >\r\n        from\r\n        <a href=\"https://www.flaticon.com/\" title=\"Flaticon\"\r\n          >www.flaticon.com</a\r\n        >\r\n      </div>\r\n    </div>\r\n\r\n    <!-- ADD PROFILE -->\r\n    <div class=\"section\">\r\n      <p class=\"header\">Add new profile:</p>\r\n      <form action=\"#\" ref=\"clearForm\" @submit.prevent=\"addProfile()\">\r\n        <div class=\"flex-row\">\r\n          <label class=\"label\">Name:</label>\r\n          <input\r\n            type=\"text\"\r\n            name=\"name\"\r\n            class=\"input\"\r\n            v-model=\"name\"\r\n            placeholder=\"Write Name\"\r\n          />\r\n        </div>\r\n        <div class=\"flex-row\">\r\n          <label class=\"label\" for=\"filter\">Email:</label>\r\n          <input\r\n            type=\"mail\"\r\n            name=\"email\"\r\n            class=\"input\"\r\n            v-model=\"email\"\r\n            placeholder=\"Write Email\"\r\n          />\r\n        </div>\r\n        <div class=\"flex-row\">\r\n          <label class=\"label\">Specialisation:</label>\r\n          <input\r\n            type=\"text\"\r\n            name=\"description\"\r\n            class=\"input\"\r\n            v-model=\"description\"\r\n            placeholder=\"Write Description\"\r\n          />\r\n        </div>\r\n        <button type=\"submit\">Add</button>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport ProfileCard from \"./components/ProfileCard\";\r\n\r\nexport default {\r\n  name: \"App\",\r\n\r\n  components: {\r\n    ProfileCard,\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      name: \"\",\r\n      email: \"\",\r\n      description: \"\",\r\n      search: \"\",\r\n      profiles: [\r\n        {\r\n          id: 1,\r\n          name: \"Wojciech\",\r\n          email: \"wojciech@poz.pl\",\r\n          description: \"Anaesthesiologist\",\r\n          likes: 34,\r\n        },\r\n        {\r\n          id: 2,\r\n          name: \"Maria\",\r\n          email: \"maria@poz.pl\",\r\n          description: \"Radiologist\",\r\n          likes: 28,\r\n        },\r\n        {\r\n          id: 3,\r\n          name: \"Anna\",\r\n          email: \"anna@poz.pl\",\r\n          description: \"Surgeon\",\r\n          likes: 53,\r\n        },\r\n      ],\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    sortAsc() {\r\n      this.profiles.sort(function (a, b) {\r\n        return a.likes - b.likes;\r\n      });\r\n    },\r\n\r\n    sortDesc() {\r\n      this.profiles.sort(function (a, b) {\r\n        return b.likes - a.likes;\r\n      });\r\n    },\r\n    // PROFILE FORM SUBMIT\r\n    addProfile() {\r\n      this.profiles.push({\r\n        name: this.name,\r\n        email: this.email,\r\n        description: this.description,\r\n      });\r\n      this.$refs.clearForm.reset();\r\n    },\r\n  },\r\n  computed: {\r\n    // SEARCH SHORTING\r\n    searchProfile() {\r\n      return this.profiles.filter((profile) => {\r\n        return profile.name.toLowerCase().match(this.search);\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n#app {\r\n  font-family: \"Roboto\", helvetica, arial, sans-serif;\r\n  text-align: center;\r\n  color: #2c3e50;\r\n  padding: 60px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  flex-direction: column;\r\n  position: relative;\r\n\r\n  background: linear-gradient(\r\n    135deg,\r\n    rgba(65, 184, 131, 0.9),\r\n    rgba(52, 73, 94, 0.9)\r\n  );\r\n\r\n  font-size: 1.5em;\r\n}\r\n\r\nbutton {\r\n  display: block;\r\n  padding: 1em;\r\n  width: 100%;\r\n  background-color: #41b883;\r\n  border: 1px solid;\r\n  color: #fff;\r\n  cursor: pointer;\r\n  font-size: 0.75em;\r\n  font-weight: 600;\r\n  text-shadow: 0 1px 0 rgba(black, 0.2);\r\n}\r\n\r\n.content {\r\n  display: flex;\r\n}\r\n\r\n.section {\r\n  width: 100%;\r\n  min-width: 300px;\r\n  padding: 2em;\r\n  margin-top: 30px;\r\n  position: relative;\r\n  background: rgba(0, 0, 0, 0.15);\r\n}\r\n\r\n@media screen and (min-width: 600px) {\r\n  .section {\r\n    width: 50vw;\r\n    max-width: 15em;\r\n  }\r\n}\r\n\r\n.header {\r\n  color: #fff;\r\n}\r\n\r\n.section::before {\r\n  content: \"\";\r\n  position: absolute;\r\n  top: -2px;\r\n  left: 0;\r\n  height: 2px;\r\n  width: 100%;\r\n  background: #35c3c1;\r\n}\r\n\r\n.flex-row {\r\n  display: flex;\r\n  margin-bottom: 1em;\r\n}\r\n\r\n.label {\r\n  width: 80px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  font-size: 12px;\r\n\r\n  background: #f5f6f8;\r\n}\r\n\r\n.input {\r\n  flex: 1;\r\n  padding: 1em;\r\n  border: 0;\r\n  color: #8f8f8f;\r\n  font-size: 1rem;\r\n}\r\n\r\n.buttons {\r\n  display: flex;\r\n  box-shadow: 0 10px 10px rgba(0, 0, 0, 0.25), 0 5px 5px rgba(0, 0, 0, 0.22);\r\n  margin-top: 30px;\r\n}\r\n\r\n.profile {\r\n  margin-top: 20px;\r\n}\r\n\r\n.icons-note {\r\n  margin-top: 30px;\r\n  font-size: 10px;\r\n}\r\n</style>\r\n"]}]}